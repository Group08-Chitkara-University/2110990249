		<div class="main">
		<div class="booking-form-box">
			<div class="radio-btn">
				<input type="radio" class="btn" name="check" checked="checked"><span>Round Trip</span>
				<input type="radio" class="btn" name="check"><span>One Way</span>
				<input type="radio" class="btn" name="check"><span>Multi-city</span>
			</div>
			<div class="booking-form">
				<div class="input-grp">
					<label>Departing From</label>
					<input type="text" class="form-control" id="one" placeholder="City/Station" required>
				</div>
				<div class="input-grp">
					<label>Departing To</label>
					<input type="text" class="form-control" id="two" placeholder="City/Station" required>
				</div>
				<div class="input-grp">
					<label>Departing Time</label>
					<input type="date" class="form-control select-date" id="three" required>
				</div>
				<div class="input-grp">
					<label>Adults</label>
					<input type="number" class="form-control" value="1" min="1" id="five" required>
				</div>
				<div class="input-grp">
					<label>Children</label>
					<input type="number" class="form-control" value="0" min="0" id="six">
				</div>
				<div class="input-grp">
					<label>Travel Class</label><br>
					<select class="custom-select" id="seven">
					<option value="1">Air-Conditioned First Class (1AC)</option>
					<option value="2">Air-Conditioned Two-Tier Class (2AC)</option>
					<option value="3">First Class (FC)</option>
					<option value="4">AC Chair Class (CC)</option>
					<option value="5">Sleeper Class (SL)</option>
					</select>
				</div>
				<div class="input-grp">
					<button type="submit" class="btn-primary">
					Show Express-Ways</button>
				</div>
			</div>
		</div>
		</div>
	</div>
